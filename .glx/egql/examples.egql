#INV-001
PATH TOPIC:gui.* >> (TOPIC:core.run.* || TOPIC:analysis.ast.delta || TOPIC:mosaic.*)
WHERE window<=5000
ORDER BY ts DESC
LIMIT 1000

# Producers of run.*
FIND topics WHERE TOPIC ~ "core.run.*" AND KIND = "define"

# Startâ†’Done without Error
PATH TOPIC:core.run.start >> TOPIC:core.run.done
WHERE NOT EXISTS TOPIC:core.run.error
